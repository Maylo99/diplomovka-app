<% content_for :page_header do %>
  <%= button_tag(type: 'submit', class: 'btn btn-primary d-flex align-items-center justify-content-center', form: 'partner_form') do %>
    <%= material_icon.shape(:save).outlined %>
    <span class="ms-2">Uložiť zmeny</span>
  <% end %>
<% end %>
<%= render 'shared/page_header', title: "Uprav partnera" %>
<% content_for :page_body do %>
  <%= render "form", partner: @partner, url: partner_path(@account, @partner)  %>
<% end %>
<%= render 'shared/page_body' %>
